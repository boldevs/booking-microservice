{
  "AppOptions": {
    "Name": "Flight-Service"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sqlexpress;Database=FlightDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Jwt": {
    "Authority": "https://localhost:5005",
    "Audience": "flight-api"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "ExchangeName": "flight",
    "UserName": "guest",
    "Password": "guest"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2",
      "Url": "https://localhost:5003"
    }
  },
  "AllowedHosts": "*"
}
